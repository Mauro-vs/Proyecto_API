<Application x:Class="Proyecto_API.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Proyecto_API"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="RacingBlack" Color="#121212"/>
        <SolidColorBrush x:Key="RacingDarkGray" Color="#1E1E1E"/>
        <SolidColorBrush x:Key="RacingRed" Color="#D50000"/>
        <SolidColorBrush x:Key="RacingTextMain" Color="#FFFFFF"/>
        <SolidColorBrush x:Key="RacingTextMuted" Color="#B0B0B0"/>
    </Application.Resources>
</Application>
